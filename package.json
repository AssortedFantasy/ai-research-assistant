{
  "name": "zotero-ra",
  "version": "0.1.6",
  "description": "Zotero Research Assistant Powered by Large Language Models",
  "config": {
    "addonName": "Zotero RA",
    "addonID": "zotero.ra@apex974.com",
    "addonRef": "zoteroRa",
    "addonInstance": "ZoteroRA",
    "releasepage": "https://github.com/lifan0127/zotero-research-assistant/releases/latest/download/zotero-ra.xpi",
    "updaterdf": "https://raw.githubusercontent.com/lifan0127/zotero-research-assistant/bootstrap/update.json"
  },
  "main": "src/index.ts",
  "scripts": {
    "build-dev": "cross-env NODE_ENV=development node scripts/build.js",
    "build-prod": "cross-env NODE_ENV=production node scripts/build.js",
    "build": "concurrently -c auto npm:build-prod npm:tsc",
    "tsc": "tsc --noEmit",
    "start-z6": "node scripts/start.js --z 6",
    "start-z7": "node scripts/start.js --z 7",
    "start": "node scripts/start.js",
    "stop": "node scripts/stop.js",
    "restart-dev": "npm run build-dev && npm run stop && npm run start",
    "restart-prod": "npm run build-prod && npm run stop && npm run start",
    "restart": "npm run restart-dev",
    "release": "release-it",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lifan0127/zotero-research-assistant.git"
  },
  "author": "Fan Li",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/lifan0127/zotero-research-assistant/issues"
  },
  "homepage": "https://github.com/lifan0127/zotero-research-assistant#readme",
  "dependencies": {
    "patch-package": "^6.5.1",
    "zotero-plugin-toolkit": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.10",
    "compressing": "^1.9.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.14",
    "minimist": "^1.2.8",
    "release-it": "^15.9.3",
    "replace-in-file": "^6.3.5",
    "typescript": "^5.0.2",
    "zotero-types": "^1.0.12"
  }
}
